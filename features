	3		pb2			sck 		sck					isp sck   pin 3
	4		pb3						oc1b = pwm 1		output 1
	5		Vcc			Vcc			Vcc					iso Vcc   pin 2
	6		GND			GND			gnd					isp gnd   pin 6
